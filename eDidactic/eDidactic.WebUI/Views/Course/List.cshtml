@using eDidactic.Domain.Entities
@model IEnumerable<Course>

@{
    ViewBag.Title = "Kursy";
}
<div>
    <h1>Hello eDidactic!</h1>
    <h2>Lista kursów:</h2>
</div>

@foreach (var p in Model){
    <div class="well">
        <h3>
            <strong>@p.Name</strong>
        </h3>
        <span class="pull-right label label-primary">@p.Description</span>
    </div>
}



